<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jfx_4_matlab.event_transfer.Controller">
   <children>
      <HBox>
         <children>
            <Button fx:id="btn_newEntry" maxWidth="1.7976931348623157E308" minWidth="-Infinity" onAction="#sendEvent" text="new entry" HBox.hgrow="ALWAYS" />
            <Button fx:id="btn_editEntry" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#sendEvent" text="edit entry" HBox.hgrow="ALWAYS" />
            <Button fx:id="btn_save" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#sendEvent" text="save" HBox.hgrow="ALWAYS" />
            <Button fx:id="btn_switchToList" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#sendEvent" text="switch to list" HBox.hgrow="ALWAYS" />
            <Button fx:id="btn_switchToPlot" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#sendEvent" text="switch to plot" HBox.hgrow="ALWAYS" />
         </children>
      </HBox>
      <TableView fx:id="table" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="500.0" prefWidth="500.0" VBox.vgrow="ALWAYS">
        <columns>
          <TableColumn fx:id="tc_name" editable="false" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefWidth="125.0" text="name" />
          <TableColumn fx:id="tc_surname" editable="false" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefWidth="125.0" text="surname" />
            <TableColumn fx:id="tc_gender" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefWidth="125.0" text="gender" />
            <TableColumn fx:id="tc_age" maxWidth="1.7976931348623157E308" minWidth="-Infinity" prefWidth="125.0" text="age" />
        </columns>
      </TableView>
   </children>
</VBox>
