<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="jfx_4_matlab.event_transfer.Controller">
   <children>
      <HBox>
         <children>
            <Button fx:id="btn_newEntry" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#sendEvent" text="new entry" HBox.hgrow="ALWAYS" />
            <Button fx:id="btn_editEntry" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#sendEvent" text="edit entry" HBox.hgrow="ALWAYS" />
            <Button fx:id="btn_save" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#sendEvent" text="save" HBox.hgrow="ALWAYS" />
            <Button fx:id="btn_switchToTable" maxWidth="1.7976931348623157E308" mnemonicParsing="false" onAction="#sendEvent" text="switch to table" HBox.hgrow="ALWAYS" />
            <Button fx:id="btn_switchToPlot" mnemonicParsing="false" onAction="#sendEvent" text="switch to plot" />
         </children>
      </HBox>
      <ListView fx:id="list" prefHeight="500.0" prefWidth="500.0" />
   </children>
</VBox>
